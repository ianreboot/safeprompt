# SafePrompt Dashboard Environment Variables
# Copy this to .env.development or .env.production and fill in values
# Or load from org-level /home/projects/.env

# Supabase Configuration (DEV)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# API Configuration
NEXT_PUBLIC_API_URL=https://api.safeprompt.dev
NEXT_PUBLIC_APP_URL=https://dev-dashboard.safeprompt.dev
NEXT_PUBLIC_WEBSITE_URL=https://dev.safeprompt.dev
NEXT_PUBLIC_DASHBOARD_URL=https://dev-dashboard.safeprompt.dev

# Stripe Configuration (test keys for dev, live keys for prod)
STRIPE_SECRET_KEY=sk_test_xxx
STRIPE_WEBHOOK_SECRET=whsec_xxx
STRIPE_PUBLISHABLE_KEY=pk_test_xxx

# Resend API key is loaded from org-level /home/projects/.env
